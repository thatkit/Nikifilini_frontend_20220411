@import '../../../../../common.styl'

.row
  min-height 64px
  display table-row

  div
    display table-cell

    div
      width 80%
      display flex
      align-items stretch
      vertical-align middle
      text-align center
    
    &:last-child div
      width 100%

.orderNumber
  font-weight 500
  color $primary-dark